{
  "name": "aurpath",
  "version": "0.1.0",
  "description": "Solana transaction outcome tracing and failure classification",
  "license": "Apache-2.0",
  "type": "module",
  "bin": {
    "aurpath": "dist/cli.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsx src/cli.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --check .",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@solana/web3.js": "^1.98.0",
    "commander": "^12.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.9.0",
    "prettier": "^3.3.0",
    "tsx": "^4.19.0",
    "typescript": "^5.6.2",
    "vitest": "^2.0.5"
  },
  "engines": {
    "node": ">=20"
  }
}
